	"devDependencies": {
		"@types/leaflet": "^1.9.0",
		"changelogkeeper": "^1.2.0",
		"esbuild": "^0.14.54",
		"esbuild-plugin-external-global": "^1.0.1",
		"express": "^4.18.2",
		"typescript": "^4.8.4"
	},
	"peerDependencies": {
		"leaflet": "^1.9.2"
